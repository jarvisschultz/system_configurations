[decor]
s0_active_shadow_color = #00000080

[expo]
s0_ground_color1 = #b3b3b3cc
s0_ground_color2 = #b3b3b300

[core]
s0_active_plugins = core;composite;opengl;decor;imgpng;gnomecompat;regex;compiztoolbox;session;resize;put;dbus;staticswitcher;mousepoll;workarounds;snap;vpswitch;resizeinfo;move;grid;wall;
s0_maximize_window_horizontally_key = <Alt>F12
s0_maximize_window_vertically_key = <Alt>F11
s0_toggle_window_maximized_key = <Alt>F10
s0_hsize = 4
s0_vsize = 1
s0_number_of_desktops = 4

[put]
s0_put_left_key = <Super>Left
s0_put_right_key = <Super>Right
s0_put_top_key = <Super>Up
s0_put_bottom_key = <Super>Down
s0_put_topleft_key = Disabled
s0_put_topright_key = Disabled
s0_put_bottomleft_key = Disabled
s0_put_bottomright_key = Disabled

[wall]
s0_show_switcher = false
s0_miniscreen = false
s0_thumb_highlight_gradient_shadow_color = #dfdfdfff
s0_arrow_base_color = #e6e6e6d9
s0_arrow_shadow_color = #dcdcdcd9

[shift]
s0_ground_color1 = #b3b3b3cc
s0_ground_color2 = #b3b3b300

[composite]
s0_refresh_rate = 50

[unityshell]
s0_show_hud = Disabled

[thumbnail]
s0_thumb_color = #0000007f

[resizeinfo]
s0_always_show = true
s0_gradient_1 = #cccce6cc
s0_gradient_2 = #f3f3f3cc
s0_gradient_3 = #d9d9d9cc

[grid]
s0_put_top_key = <Control><Primary><Super>Up
s0_put_bottom_key = <Control><Primary><Super>Down
s0_put_topleft_key = Disabled
s0_put_topright_key = Disabled
s0_put_bottomleft_key = Disabled
s0_put_bottomright_key = Disabled
s0_put_restore_key = Disabled

